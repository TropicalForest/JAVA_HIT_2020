package b1;

public class Btap1_Buoi1 {
	public static void main(String[] args) {
		int m = 5, n = 3;
		for(int i = 1; i <= m; ++i) {
			System.out.print("*");
			for(int j = 1; j <= n; ++j) {
				if(i==1 || i==m) {
					System.out.print("* ");
				}
				else {
					System.out.print(" ");
				}
			}
			System.out.println("*");
		}
	}
}
